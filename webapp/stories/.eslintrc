{
  "extends": "pagarme-react",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "import/no-extraneous-dependencies": [0],
    "react/jsx-filename-extension": 0,
    "react/prop-types": [0],
    "react/jsx-no-bind": [
      2
    ]
  }
}
